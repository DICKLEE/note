stop_words = []
regex = re.compile('(\w)+')

with open(path_f, "rb") as f:
    content = f.readlines()

for i in content:
    
    temp = i.decode("utf8")
    if regex.search(temp) is None:
        stop_words.append(temp.replace("\n",""))
    else:
        stop_words.append(regex.search(temp).group())
        
        
with open("stop_words.txt", "w") as output:
    for i in stop_words:
        output.write(i+"\n")
